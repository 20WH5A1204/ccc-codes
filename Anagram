ef isAnagram(s1,s2):

    n1 = len(s1)
    n2 = len(s2)

    if n1 != n2:
        return 0
     
    s1 = sorted(s1)
    s2 = sorted(s2)

    for i in range(0 , n1):
        if s1[i] != s2[i]:
            return 0
    
    return 1
s1 = str(input())
s2 = str(input())

if isAnagram(s1 , s2):
    print("Anagram")
else:
    print("Not Anagram")
